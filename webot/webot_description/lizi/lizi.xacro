<!-- Wheels friction -->

<gazebo reference="FR_Wheel_link">
    <mu1>1</mu1>
    <mu2>1</mu2>
    <!--selfCollide>true</selfCollide-->
    <material>Gazebo/Grey</material>
</gazebo>
<gazebo reference="FL_Wheel_link">
    <mu1>1</mu1>
    <mu2>1</mu2>
    <!--selfCollide>true</selfCollide-->
